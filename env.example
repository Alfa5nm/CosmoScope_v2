# Environment Variables for CosmoScope
# Copy this file to .env and fill in your values

# NASA API Configuration
NASA_API_KEY=your_nasa_api_key_here

# Server Configuration
PORT=5174
NODE_ENV=development

# CORS Configuration
ALLOWED_ORIGINS=http://localhost:5173,http://localhost:5175,http://localhost:5176

# Database Configuration
DATABASE_PATH=./db.sqlite

# Cache Configuration
TILE_CACHE_ENABLED=false
TILE_CACHE_DIR=./tile-cache
TILE_CACHE_TTL=3600
TILE_CACHE_MAX_ENTRY_BYTES=2097152

# Redis Configuration (Optional)
TILE_CACHE_REDIS_URL=redis://localhost:6379
TILE_CACHE_NAMESPACE=cosmoscope_tiles

# IIIF Server Configuration
IIIF_SERVER_PORT=8080
