{
  "name": "cosmoscope",
  "version": "1.0.0",
  "description": "CosmoScope - Vectorized 3D Solar System Explorer for NASA Space Apps 2025",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:client": "cd client && vite",
    "dev:server": "tsx watch server/server.ts",
    "dev:full": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:cantaloupe": "cd server && node simple-image-server.js",
    "dev:iiif": "cd server && node iiif-server.js",
    "dev:with-images": "concurrently \"npm run dev:cantaloupe\" \"npm run dev:iiif\" \"npm run dev:server\" \"npm run dev:client\"",
    "build": "npm run build:client && npm run build:server",
    "build:client": "cd client && vite build",
    "build:server": "cd server && tsc",
    "build:demo": "cd client && vite build --mode demo",
    "start": "node server/dist/server.js",
    "start:demo": "node server/dist/server.js --demo",
    "start:production": "NODE_ENV=production node server/dist/server.js",
    "install:all": "npm install && cd server && npm install",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "typecheck": "tsc --noEmit",
    "demo": "npm run build:demo && npm run start:demo"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.17.0",
    "better-sqlite3": "^9.2.2",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "framer-motion": "^10.16.16",
    "ioredis": "^5.3.2",
    "maplibre-gl": "^3.6.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.1",
    "three": "^0.158.0",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.8",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.5",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^8.2.2",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "tsx": "^4.6.2",
    "typescript": "^5.2.2",
    "vite": "^5.0.8"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "nasa",
    "space",
    "3d",
    "solar-system",
    "exploration",
    "data-visualization",
    "webgl",
    "threejs",
    "maplibre",
    "react",
    "typescript"
  ],
  "author": "NASA Space Apps 2025 Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/cosmoscope"
  },
  "bugs": {
    "url": "https://github.com/your-username/cosmoscope/issues"
  },
  "homepage": "https://github.com/your-username/cosmoscope#readme"
}